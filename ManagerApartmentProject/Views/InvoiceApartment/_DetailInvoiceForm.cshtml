@model IEnumerable<DetailInvoice>

@{
    if (Model == (null))
    {
        @await Html.PartialAsync(
            "_DetailInvoiceRow.cshtml",
            new DetailInvoice(),
            ViewData
        )
    }
    else
    {
        @for (var i = 0; i < Model.Count<DetailInvoice>(); i++)
        {
            @await Html.PartialAsync(
                "_DetailInvoiceRow.cshtml",
                Model.ElementAt(i),
                ViewData
            )
        }
    }
}

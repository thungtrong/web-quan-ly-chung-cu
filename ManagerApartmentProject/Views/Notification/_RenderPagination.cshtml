<nav class="@Model.name" aria-label="Page navigation">
  <ul class="pagination">

    @{
      int start = Model.start - 1;
      int end = Model.end;

      <li class="page-item">
        <a class="page-link" href="#" data-page="@start" aria-label="Previous">
          <span aria-hidden="true">&laquo;</span>
        </a>
      </li>
      
      start += 1;

      <li class="page-item active"><a class="page-link" href="#" data-page="@start">@start</a></li>

      for (start = start + 1; start <= end; start++)
      {
        <li class="page-item"><a class="page-link" href="#" data-page="@start">@start</a></li>
      }
    }
    <li class="page-item">
      <a class="page-link" href="#" data-page="@start" aria-label="Next">
        <span aria-hidden="true">&raquo;</span>
      </a>
    </li>
  </ul>
</nav>
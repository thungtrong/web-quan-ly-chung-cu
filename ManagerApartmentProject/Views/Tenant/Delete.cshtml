@model Tenant

<form asp-action="Delete" method="post">
    <input asp-for="ID" type="hidden" />
    <div class="form-group">
        <label asp-for="name"></label>
        <input asp-for="name" class="form-control" disabled placeholder="Nhập tiêu đề">
        <span asp-validation-for="name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="email"></label>
        <input asp-for="email"  class="form-control" disabled placeholder="Nhập tiêu đề">
        <span asp-validation-for="email" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="phoneNumber"></label>
        <input asp-for="phoneNumber"  class="form-control" disabled placeholder="Nhập tiêu đề">
        <span asp-validation-for="phoneNumber" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="advancePayment"></label>
        <input asp-for="advancePayment"  class="form-control" disabled placeholder="Nhập tiêu đề">
        <span asp-validation-for="advancePayment" class="text-danger"></span>
    </div>
   
    
    <div class="form-group">
        <button type="submit" class="btn btn-primary submit">Delete</button>
        <button class="btn btn-default" asp-action="Index">Back</button>
    </div>
</form>